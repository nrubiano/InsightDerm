{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "clean-ui": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              "src/assets",
              "src/favicon.ico"
            ],
            "styles": [
              "src/styles.css",
              "src/assets/vendors/bootstrap/dist/css/bootstrap.min.css",
              "src/assets/vendors/jscrollpane/style/jquery.jscrollpane.css",
              "src/assets/vendors/ladda/dist/ladda-themeless.min.css",
              "src/assets/vendors/bootstrap-select/dist/css/bootstrap-select.min.css",
              "src/assets/vendors/select2/dist/css/select2.min.css",
