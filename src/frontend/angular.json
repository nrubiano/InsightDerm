              "src/assets/modules/ecommerce/css/ecommerce.cleanui.css",
              "src/assets/modules/apps/css/apps.cleanui.css",
              "node_modules/font-awesome/css/font-awesome.min.css",
              "node_modules/devextreme/dist/css/dx.common.css",
              "node_modules/devextreme/dist/css/dx.material.blue.light.css"
            ],
            "scripts": [
              "src/assets/vendors/jquery/dist/jquery.js",
              "src/assets/vendors/tether/dist/js/tether.min.js",
              "src/assets/vendors/jquery-ui/jquery-ui.min.js",
              "src/assets/vendors/bootstrap/dist/js/bootstrap.min.js",
              "src/assets/vendors/jquery-mousewheel/jquery.mousewheel.min.js",
